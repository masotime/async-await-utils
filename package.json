{
  "name": "async-await-utils",
  "version": "1.0.0",
  "description": "A series of utilities to make programming with async/await easier",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/*",
    "compile": "rm -rf dist/; babel --plugins transform-runtime,add-module-exports -d dist/ src/",
    "prepublish": "npm test && npm run compile",
    "test": "npm run lint; BABEL_ENV=test nyc tape test/**/*.js | tap-spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:masotime/async-await-utils.git"
  },  
  "author": "Benjamin Goh <masotime@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-module-resolver": "^2",
    "babel-preset-es2015": "^6",
    "babel-preset-stage-0": "^6",
    "babel-runtime": "^6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7",
    "babel-plugin-add-module-exports": "^0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "eslint": "^3",
    "eslint-import-resolver-babel-module": "^2",
    "eslint-plugin-import": "^2",
    "nyc": "^9.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  }
}
